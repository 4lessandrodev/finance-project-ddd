{
	"name": "finance-project",
	"version": "0.1.0",
	"description": "",
	"author": "Alessandro L. Menezes",
	"private": true,
	"license": "MIT",
	"scripts": {
		"prestart:dev": "lsof -i:3000 | awk '/node/{print $2}' | awk 'NR == 1' | xargs -r kill -9 ",
		"postinstall": "./scripts/post-install.sh",
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "env-cmd nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "NODE_ENV=production node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "TYPES_DDD_LOGS=off jest --runInBand --silent -c ./jest.unit.config.js",
		"test:verbose": "jest --runInBand -c ./jest.unit.config.js",
		"test:watch": "jest --watch -c ./jest.unit.config.js",
		"test:cov": "TYPES_DDD_LOGS=off jest --coverage --runInBand -c ./jest.unit.config.js",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand -c ./jest.unit.config.js",
		"test:e2e": "TYPES_DDD_LOGS=off jest -c ./jest.e2e.config.js --runInBand",
		"generate:gql": "graphql-codegen --config ./code-gen.yml"
	},
	"dependencies": {
		"@apollo/gateway": "^0.34.0",
		"@nestjs/common": "^8.1.2",
		"@nestjs/core": "^8.1.2",
		"@nestjs/graphql": "^8.0.2",
		"@nestjs/jwt": "^8.0.0",
		"@nestjs/mongoose": "^8.0.0",
		"@nestjs/passport": "^8.0.0",
		"@nestjs/platform-express": "^8.1.2",
		"apollo-server-express": "2.25.2",
		"bcrypt": "^5.0.1",
		"date-fns": "2.23.0",
		"env-cmd": "^10.1.0",
		"env-var": "^7.1.1",
		"graphql": "^15.7.2",
		"graphql-tools": "8.2",
		"module-alias": "^2.2.2",
		"mongoose": "5.13.3",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "7.2.0",
		"ts-morph": "^11.0.3",
		"types-ddd": "^2.10.0",
		"ua-parser-js": "0.7.31",
		"uuid": "^8.3.2",
