		"@graphql-codegen/typescript-resolvers": "^2.5.2",
		"@microsoft/tsdoc": "0.13.2",
		"@nestjs/cli": "8.0.2",
		"@nestjs/schematics": "8.0.2",
		"@nestjs/testing": "8.0.4",
		"@types/bcrypt": "5.0.0",
		"@types/express": "^4.17.8",
		"@types/faker": "5.5.7",
		"@types/jest": "26.0.24",
		"@types/module-alias": "^2.0.0",
